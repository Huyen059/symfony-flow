{% extends 'base.html.twig' %}

{% block title %}Manager Dashboard{% endblock %}

{% block body %}
    <h1>Table of all Customers</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ customer.id }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ customer.email }}</td>
        </tr>
        <tr>
            <th>IsVerified</th>
            <td>{{ customer.isVerified ? 'Yes' : 'No' }}</td>
        </tr>
        </tbody>
    </table>

    <h1>Table of all Agents</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ agent.id }}</td>
        </tr>
        <tr>
            <th>Email</th>
            <td>{{ agent.email }}</td>
        </tr>
        <tr>
            <th>IsVerified</th>
            <td>{{ agent.isVerified ? 'Yes' : 'No' }}</td>
        </tr>
        </tbody>
    </table>


    {{ include('user/_delete_form.html.twig') }}
{% endblock %}
